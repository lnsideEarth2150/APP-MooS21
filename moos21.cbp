<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="moos21" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/debug/moos21" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/debug" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/release/moos21" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/release" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fomit-frame-pointer" />
					<Add option="-fexpensive-optimizations" />
					<Add option="-O2" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static-libgcc" />
					<Add option="-static-libstdc++" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd.exe /c start /min /belownormal upx --ultra-brute $(TARGET_OUTPUT_FILE) -f -o $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME)_upx.exe" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
		</Compiler>
		<Linker>
			<Add library="wxmsw30u_richtext" />
			<Add library="wxmsw30u_html" />
			<Add library="wxmsw30u_aui" />
			<Add library="wxmsw30u_adv" />
			<Add library="wxmsw30u_core" />
			<Add library="wxbase30u" />
			<Add library="wxbase30u_net" />
			<Add library="wxpng" />
			<Add library="wxzlib" />
			<Add library="comctl32" />
			<Add library="rpcrt4" />
			<Add library="shell32" />
			<Add library="gdi32" />
			<Add library="kernel32" />
			<Add library="user32" />
			<Add library="comdlg32" />
			<Add library="ole32" />
			<Add library="oleaut32" />
			<Add library="advapi32" />
			<Add library="winspool" />
			<Add library="winmm" />
			<Add library="uuid" />
			<Add library="wsock32" />
			<Add library="z" />
		</Linker>
		<Unit filename="base.cpp" />
		<Unit filename="base.h" />
		<Unit filename="connection.cpp" />
		<Unit filename="enums.h" />
		<Unit filename="helpmenu.cpp" />
		<Unit filename="infodialog.cpp" />
		<Unit filename="infodialog.h" />
		<Unit filename="inputdialog.cpp" />
		<Unit filename="inputdialog.h" />
		<Unit filename="moos.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="moos21_private.h" />
		<Unit filename="moosmenu.cpp" />
		<Unit filename="settingsmenu.cpp" />
		<Unit filename="tools.cpp" />
		<Unit filename="tools.h" />
		<Unit filename="usermenu.cpp" />
		<Unit filename="viewmenu.cpp" />
		<Unit filename="whoisdialog.cpp" />
		<Unit filename="whoisdialog.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
