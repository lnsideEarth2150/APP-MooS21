<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="moos21" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/debug/moos21" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/debug" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-Wno-deprecated-declarations" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/moos21" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/release" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-march=prescott" />
					<Add option="-fomit-frame-pointer" />
					<Add option="-fexpensive-optimizations" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static-libgcc" />
					<Add option="-static-libstdc++" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd.exe /c start /min /belownormal upx --ultra-brute $(TARGET_OUTPUT_FILE) -f -o $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME)_upx.exe" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="`wx-config-3.0 --cppflags`" />
		</Compiler>
		<Linker>
			<Add option="`wx-config-3.0 --libs`" />
			<Add library="z" />
		</Linker>
		<Unit filename="src/base.cpp" />
		<Unit filename="src/base.h" />
		<Unit filename="src/connection.cpp" />
		<Unit filename="src/enums.h" />
		<Unit filename="src/helpmenu.cpp" />
		<Unit filename="src/infodialog.cpp" />
		<Unit filename="src/infodialog.h" />
		<Unit filename="src/inputdialog.cpp" />
		<Unit filename="src/inputdialog.h" />
		<Unit filename="src/moos21_private.h" />
		<Unit filename="src/moosmenu.cpp" />
		<Unit filename="src/settingsmenu.cpp" />
		<Unit filename="src/tools.cpp" />
		<Unit filename="src/tools.h" />
		<Unit filename="src/usermenu.cpp" />
		<Unit filename="src/viewmenu.cpp" />
		<Unit filename="src/whoisdialog.cpp" />
		<Unit filename="src/whoisdialog.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
